# EASY-INSTALL-ENTRY-SCRIPT: 'pep8==1.6.2','console_scripts','pep8'
__requires__ = 'pep8==1.6.2'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('pep8==1.6.2', 'console_scripts', 'pep8')()
    )
